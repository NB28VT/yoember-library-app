<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
      <th></th>      
    </tr>
  </thead>

  <tbody>
    {{#each model as |contact| }}
      <tr>
        <th>{{contact.email}}</th>
        <th>{{contact.message}}</th>
        <th><button class="btn btn-danger"{{action 'deleteContact' contact}}>Remove</button></th>

      </tr>
    {{/each}}
  </tbody>
</table>